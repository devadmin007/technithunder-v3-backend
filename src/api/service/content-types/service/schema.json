      "repeatable": false
    },
    "segmentsWeServe": {
      "type": "component",
      "component": "v1.segments-we-serve-component",
      "repeatable": false
    },
    "servicesInfo": {
      "type": "component",
      "component": "v1.services-info-component",
      "repeatable": false
    },
    "servicePartner": {
      "type": "component",
      "component": "v1.service-partner-component",
      "repeatable": false
    },
    "faqs": {
      "type": "component",
      "component": "v1.que-ans-component",
      "repeatable": true
